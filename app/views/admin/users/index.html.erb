<% @users.each do |user| %>
  <div>
    <p>ユーザー名: <%= user.user_name %></p>
    <p>メールアドレス: <%= user.email %></p>
    <% if user.is_deleted == false %>
      <p>有効</p>
      <%= link_to '退会させる', public_users_quit_path, method: :patch, data: { confirm: '本当に退会させますか？' } %>
    <% else %>
      <p>退会</p>
  <%end%>
    
  </div>
<% end %>